{
  "name": "vite-plugin-dynamic-import",
  "version": "1.2.1",
  "description": "Enhance Vite builtin dynamic import",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vite-plugin/vite-plugin-dynamic-import.git"
  },
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node build.js --watch",
    "dev:es": "vite build --watch",
    "dev:cjs": "tsc --watch",
    "build": "node build.js",
    "build:es": "vite build",
    "build:cjs": "tsc",
    "test": "vite -c test/vite.config.ts",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "fast-glob": "^3.2.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "typescript": "^4.6.4",
    "vite": "^2.9.9"
  },
  "keywords": [
    "vite",
    "plugin",
    "import",
    "dynamic"
  ],
  "files": [
    "dist",
    "src"
  ]
}
